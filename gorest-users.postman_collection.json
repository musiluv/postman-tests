		{
			"name": "create",
			"item": [
				{
					"name": "Create new user",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Successful POST request\", function () {\r",
									"    pm.response.to.have.status(201);\r",
									"});\r",
									"\r",
									"pm.test(\"Verify response headers\", function(){\r",
									"    var headers = pm.response.headers;\r",
									"\r",
									"    pm.response.to.have.header(\"Content-Type\");\r",
									"    pm.expect(headers.get('Content-Type')).to.eql('application/json; charset=utf-8');\r",
									"\r",
									"    pm.response.to.have.header(\"Connection\");\r",
									"    pm.expect(headers.get('Connection')).to.eql('keep-alive');\r",
									"\r",
									"    pm.response.to.have.header(\"Cache-Control\");\r",
									"    pm.expect(headers.get('Cache-Control')).to.eql('max-age=0, private, must-revalidate');\r",
									"\r",
									"    pm.response.to.have.header(\"Strict-Transport-Security\");\r",
									"    pm.expect(headers.get('Strict-Transport-Security')).to.eql('max-age=63072000; includeSubDomains');\r",
									"\r",
									"    pm.response.to.have.header(\"Transfer-Encoding\");\r",
									"    pm.expect(headers.get('Transfer-Encoding')).to.eql('chunked');\r",
									"});\r",
									"\r",
									"pm.test(\"Response includes correct data fields\", function(){\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData).to.be.an(\"object\");\r",
									"    \r",
									"    pm.expect(jsonData).to.include.all.keys('id', 'name', 'email','gender','status');\r",
									"     \r",
									"    pm.expect(jsonData.id).to.be.finite;\r",
									"    pm.expect(jsonData.name).to.be.a('string');\r",
									"    pm.expect(jsonData.email).to.be.a('string');\r",
									"    pm.expect(jsonData.gender).to.be.a('string');\r",
									"    pm.expect(jsonData.status).to.be.a('string');\r",
									"});\r",
									"\r",
									"pm.test(\"POST response data valid\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    \r",
									"    pm.globals.set(\"id\", jsonData.id);\r",
									"    \r",
									"    pm.expect(jsonData.name).to.eql(pm.globals.get(\"name\"));\r",
									"    pm.expect(jsonData.email).to.eql(pm.globals.get(\"email\"));\r",
									"    pm.expect(jsonData.gender).to.eql(pm.globals.get(\"gender\"));\r",
									"    pm.expect(jsonData.status).to.eql(pm.globals.get(\"status\"));\r",
									"\r",
									"});"
								],
								"type": "text/javascript"
							}
						}
